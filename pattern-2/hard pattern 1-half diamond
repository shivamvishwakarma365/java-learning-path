#include<iostream>
using namespace std;

int main() {
	int n;
    cin>>n;
    int m=n+1;
    int i;
    int j;
    int k;
	if(n!=0){
    for(i=0;i<m;i++){
        if(i==0){
            cout<<"*";
            
        }else{
            cout<<"*";
            for(j=1;j<=i;j++){
                cout<<j;
            }
            for(k=j-2;k>=1;k--){
                cout<<k;
            }
            cout<<"*";
        }
        cout<<endl;
    }
    for(i=i-2;i>=0;i--){
         if(i==0){
             cout<<"*";
         }else{
             cout<<"*";
             for(j=1;j<=i;j++){
                 cout<<j;
             }
             for(k=j-2;k>=1;k--){
                 cout<<k;
             }
             cout<<"*";
         }
         cout<<endl;
    }
	}else{
		cout<<"*"<<endl;
		cout<<"*";
	}
}
