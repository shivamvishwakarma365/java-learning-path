import java.util.*;
class HelloWorld {
    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
      // we will be using explicit type conversion
      int n=s.nextInt();
      for(int i=0;i<n;i++){
          for(int j=0;j<n;j++){
              System.out.print((char)('A'+j));
          }
          System.out.print("\n");
      }
      
    }
}
