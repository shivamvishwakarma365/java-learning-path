import java.util.Scanner;
public class Solution {


	public static void main(String[] args) {
		
		Scanner s=new Scanner(System.in);
		int n=s.nextInt();

		for(int i=1;i<=n;i++){
			char a=(char)('A'+i-1);
			for(int j=1;j<=i;j++){
				System.out.print(a);
				a=(char)(a+1);
			}
			System.out.print("\n");

		}

		
	}

}
